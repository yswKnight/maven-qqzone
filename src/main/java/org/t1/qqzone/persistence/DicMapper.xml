<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.t1.qqzone.persistence.DicMapper">

	<select id="findQuestionsAll" resultType="Dic">
		select * from dic
	</select>
	
	<select id="getDicById" resultType="Dic">
		select * from dic where dicid=#{dicid}
	</select>
	
</mapper>
